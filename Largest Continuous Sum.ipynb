   "source": [
    "def large_sum(arr):\n",
    "    max_sum = cur_sum = arr[0]\n",
    "    \n",
    "    for num in arr[1:]:\n",
    "        cur_sum = max(cur_sum+num,num)\n",
    "        max_sum = max(max_sum,cur_sum)\n",
    "    return max_sum"
   ]
  },
